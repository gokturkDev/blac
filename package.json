{
  "name": "bloc-react",
  "version": "0.1.19",
  "license": "MIT",
  "main": "dist/bloc-react.js",
  "module": "dist/bloc-react.esm.js",
  "typings": "dist/bloc-react.d.ts",
  "keywords": [
    "bloc",
    "state",
    "react",
    "reactive"
  ],
  "scripts": {
    "dev": "vite",
    "dev:tool": "cd devtools && vite",
    "build": "tsc && vite build",
    "build:tool": "yarn build:scripts && cd devtools && vite build",
    "build:lib": "rollup -c rollup.config.lib.js",
    "serve": "vite preview",
    "prettier": "prettier --write ./src",
    "test": "jest",
    "build:scripts": "cd devtools && rollup -c rollup.config.background.js && rollup -c rollup.config.body.js && rollup -c rollup.config.inject.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/chrome": "^0.0.158",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^26.0.23",
    "@types/material-ui": "^0.21.9",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "codecov": "^3.8.3",
    "enzyme": "^3.11.0",
    "esbuild": "^0.13.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "ext-messenger": "^3.0.2",
    "install": "^0.13.0",
    "jest": "26.6.3",
    "jest-localstorage-mock": "^2.4.18",
    "jest-mock-console": "^1.2.3",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "rollup": "^2.58.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.3",
    "vite": "^2.6.4"
  },
  "jest": {
    "preset": "ts-jest",
    "resetMocks": false,
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "jest-mock-console/dist/setupTestFramework.js",
      "./setupTests.ts"
    ],
    "collectCoverageFrom": [
      "src/lib/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
