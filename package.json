    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rollup": "^2.45.2",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-esbuild": "^4.1.0",
    "typescript": "^4.2.4",
    "vite": "^2.2.1"
