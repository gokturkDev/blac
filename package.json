    "@material-ui/icons": "^4.11.2",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/material-ui": "^0.21.8",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
