    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.23.2",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.10",
    "jest-mock-console": "^1.0.1",
    "npm": "^7.10.0",
    "prettier": "^2.2.1",
